    <!-- Google OAuth handling: -->


    <script>
      function onSignIn(googleUser) {
        // Useful data for your client-side scripts:
        var profile = googleUser.getBasicProfile();

     
        

        document.querySelector(".nameIn").setAttribute("value", profile.getGivenName());

        // The ID token you need to pass to your backend:
        var id_token = googleUser.getAuthResponse().id_token;
        console.log("ID Token: " + id_token);


        var element = document.querySelector("#content");
        var element2 = document.querySelector("#content2");
        element.innerText="Welcome, "+profile.getGivenName()+"!";

        var image = document.createElement("img");
        image.setAttribute("src", profile.getImageUrl());
        image.classList.add("googleImage")
        element2.append(image);

        
      }
      </script>





